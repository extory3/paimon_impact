\n    },\n    onRest: () => {\n      if (isGone) {\n        setDvalue(getPath(0, 0, w));\n      }\n    },\n  }));\n  useEffect(() => {\n    if (!gone) {\n      setDvalue({\n        d: getPath(height * 0.72, 48, 5),\n      });\n      setTimeout(() => {\n        setPos({\n          posX: 7,\n        });\n      }, 100);\n    }\n  }, [gone]);\n\n  const bind = useDrag(({ down, movement: [mx], xy: [, my], vxvy: [vx] }) => {\n    if (!isGone) {\n      if (down && isMove) {\n        setDvalue({\n          d: getPath(my, mx + 60, 10),\n        });\n        setPos({\n          posX: mx + 20,\n          posY: my - 20,\n        });\n        if (mx > width / 2 || vx > 3) {\n          setDvalue({\n            d: getPath(my, -50, w),\n          });\n          setGone(true);\n          setTimeout(() => {\n            setDvalue({\n              d: getPath(my, 0, w),\n            });\n            setActive(index);\n          }, 240);\n        }\n      } else {\n        setDvalue({\n          d: getPath(height * 0.72, 48, 5),\n        });\n        setPos({\n          posX: 7,\n          posY: height * 0.72 - 20,\n        });\n      }\n    }\n  });\n  return (\n    <PageContainer id={`pageContainer${index}`} {...bind()}>\n      <StyledSVG version=\"1.1\" id=\"blob\" xmlns=\"http://www.w3.org/2000/svg\">\n        <clipPath id={`clipping${index}`}>\n          <animated.path id={`blob-path${index}`} d={d} />\n        </clipPath>\n      </StyledSVG>\n      <PageDiv\n        style={{\n          clipPath: `url(#clipping${index})`,\n          WebkitClipPath: `url(#clipping${index})`,\n        }}\n      >\n        {children}\n      </PageDiv>\n      <StyledButton\n        id={`button${index}`}\n        color={theme}\n        onMouseDown={() => {\n          setMove(true);\n        }}\n        onMouseUp={() => {\n          setMove(false);\n        }}\n        onTouchStart={() => {\n          setMove(true);\n        }}\n        onTouchEnd={() => {\n          setMove(false);\n        }}\n        