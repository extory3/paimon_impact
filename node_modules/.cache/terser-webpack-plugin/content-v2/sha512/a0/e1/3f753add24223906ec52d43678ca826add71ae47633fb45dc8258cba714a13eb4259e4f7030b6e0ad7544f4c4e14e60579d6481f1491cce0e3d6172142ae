h(\"#fff\");\n  colors.push(\"#FCF6F5FF\");\n\n  return (\n    <div className=\"App\">\n      <LiquidSwipe components={components} colors={colors} />\n    </div>\n  );\n}\n\nexport default App;\n","import React from \"react\";\nimport ReactDOM from \"react-dom\";\nimport \"./index.css\";\nimport App from \"./App\";\n\nReactDOM.render(<App />, document.getElementById(\"root\"));\n"]}}